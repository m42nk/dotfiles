[submodule ".meta/dotbot"]
	path = .meta/dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule "dotfiles-nvim"]
	path = packages/nvim
	url = https://github.com/m42nk/dotfiles-nvim
	branch = master
	update = rebase
[submodule "dotfiles-tmux"]
	path = packages/tmux
	url = https://github.com/m42nk/dotfiles-tmux
	branch = master
	update = rebase
[submodule "dotfiles-secret"]
  path = packages/secrets
	url = https://github.com/m42nk/dotfiles-secret
	branch = master
	update = rebase

