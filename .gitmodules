[submodule "dotfiles-nvim"]
	path = nvim
	url = https://github.com/m42nk/dotfiles-nvim
	ignore = dirty
	branch = master
	update = rebase
[submodule "dotfiles-tmux"]
	path = tmux
	url = https://github.com/m42nk/dotfiles-tmux
